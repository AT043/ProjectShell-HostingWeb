;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	nao.net. root.nao.net. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	nao.net.
7	IN	PTR	nao.net.
7	IN	PTR	www.nao.net.
7	IN	PTR	pma.nao.net.
7	IN	PTR	file.nao.net.
